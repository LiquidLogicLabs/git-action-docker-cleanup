{
  "name": "git-action-docker-cleanup",
  "version": "1.0.4",
  "description": "Multi-registry container cleanup action for GHCR, Gitea, Docker Hub, and OCI-compliant registries",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc && ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package": "npm run build",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns='integration|e2e'",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e --testTimeout=30000",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --no-warn-ignored",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:act": "act --container-options \"--user $(id -u):$(id -g)\" workflow_dispatch -W .github/workflows/test.yml --eventpath .github/workflows/.act/event-workflow_call.json --env-file .act.env --secret-file .act.secrets --var-file .act.vars",
    "test:act:verbose": "act --container-options \"--user $(id -u):$(id -g)\" workflow_dispatch -W .github/workflows/test.yml --eventpath .github/workflows/.act/event-workflow_call.json --env-file .act.env --secret-file .act.secrets --var-file .act.vars -v",
    "test:act:ci": "act --container-options \"--user $(id -u):$(id -g)\" -W .github/workflows/ci.yml --env-file .act.env --secret-file .act.secrets --var-file .act.vars",
    "test:act:release": "act --container-options \"--user $(id -u):$(id -g)\" push --eventpath .github/workflows/.act/event-release.json --env-file .act.env --secret-file .act.secrets --var-file .act.vars",
    "test:act:e2e": "act --container-options \"--user $(id -u):$(id -g)\" workflow_dispatch -W .github/workflows/e2e-tests.yml --eventpath .github/workflows/.act/event-e2e-tests.json --env-file .act.env --secret-file .act.secrets --var-file .act.vars",
    "test:act:e2e:verbose": "act --container-options \"--user $(id -u):$(id -g)\" workflow_dispatch -W .github/workflows/e2e-tests.yml --eventpath .github/workflows/.act/event-e2e-tests.json --env-file .act.env --secret-file .act.secrets --var-file .act.vars -v",
    "lint:act": "act --container-options \"--user $(id -u):$(id -g)\" -W .github/workflows/ci.yml -j lint --env-file .act.env --secret-file .act.secrets --var-file .act.vars",
    "version:dry-run": "standard-version --dry-run",
    "version:patch": "standard-version --release-as patch",
    "version:minor": "standard-version --release-as minor",
    "version:major": "standard-version --release-as major",
    "release:push": "git push --follow-tags origin HEAD",
    "release:patch": "npm run version:patch && npm run release:push",
    "release:minor": "npm run version:minor && npm run release:push",
    "release:major": "npm run version:major && npm run release:push",
    "test:act:ci:push": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/ci.yml --eventpath .github/workflows/.act/event-push.json",
    "test:act:ci:dispatch": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/ci.yml --eventpath .github/workflows/.act/event-workflow_dispatch.json",
    "test:act:ci:with-secrets": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/ci.yml",
    "test:act:e2e:dispatch": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/e2e-tests.yml --eventpath .github/workflows/.act/event-workflow_dispatch.json",
    "test:act:e2e:with-secrets": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/e2e-tests.yml",
    "test:act:release:tag": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/release.yml --eventpath .github/workflows/.act/event-release.json",
    "test:act:release:dispatch": "act --container-options \"--user $(id -u):$(id -g)\" --env-file .act.env --var-file .act.vars --secret-file .act.secrets -W .github/workflows/release.yml --eventpath .github/workflows/.act/event-workflow_dispatch.json"
  },
  "keywords": [
    "github-actions",
    "container-registry",
    "cleanup",
    "docker",
    "ghcr",
    "gitea"
  ],
  "author": "LiquidLogicLabs",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "husky": "^9.1.7",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.3",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "tagPrefix": "v"
  }
}
